
<div class="bg">
  <div class="container">
    <div class="login_form">
      <%= form_tag sessions_path do |f| %>
        <%= label_tag :username%>
        <%= text_field_tag :username, :class => 'form_tag'%>

        <%= label_tag :password %>
        <%= password_field_tag :password%>

        <%= submit_tag :Login, :class => 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>
